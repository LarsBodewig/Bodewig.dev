{
    "editor.formatOnSave": true,
    "html.format.indentInnerHtml": true,
    "html.format.extraLiners": "body, main, footer",
    "files.associations": {
        "*.liquid": "html"
    },
    "files.exclude": {
        "node_modules": true
    },
    "files.insertFinalNewline": true,
    "prettier.tabWidth": 4,
}